    "def string_rev(revstring):\n",
    "    i = len(revstring)-1\n",
    "    while i>=0:\n",
    "        n=''\n",
    "        n = n + revstring[i]\n",
    "        print(n)\n",
    "        i = i-1\n",
    "    return n"
   ]
